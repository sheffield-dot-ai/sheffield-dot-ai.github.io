{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "plugins": [
      {
        "name": "typescript-plugin-css-modules",
        "options": {
          "classnameTransform": "camelCaseOnly"
        }
      }
    ],
    "paths": {
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "gatsby-node.ts",
    "gatsby-config.ts"
  ],
  "exclude": [
    "node_modules/*",
    "public/*"
  ]
}